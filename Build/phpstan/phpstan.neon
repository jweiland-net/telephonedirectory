includes:
	- phpstan-baseline.neon
parameters:
	level: 3

	inferPrivatePropertyTypeFromConstructor: true
	treatPhpDocTypesAsCertain: false

	bootstrapFiles:
		- phpstan-typo3-constants.php

	paths:
		- ../../Classes/
		- ../../Configuration/

	tmpDir: ../../.Build/.cache/phpstan/

	excludePaths:
		- '../../ext_emconf.php'
