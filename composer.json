{
	"name": "jweiland/telephonedirectory",
	"description": "Telephone Directory",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"keywords": [
		"typo3",
		"TYPO3 CMS",
		"telephonedirectory",
		"telephone",
		"directory"
	],
	"authors": [
		{
			"name": "Stefan Froemken",
			"email": "projects@jweiland.net",
			"role": "Lead Developer"
		},
		{
			"name": "Hoja Mustaffa Abdul Latheef",
			"email": "projects@jweiland.net",
			"role": "Senior Developer"
		}
	],
	"homepage": "https://jweiland.net",
	"support": {
		"email": "projects@jweiland.net",
		"issues": "https://github.com/jweiland-net/telephonedirectory/issues",
		"source": "https://github.com/jweiland-net/telephonedirectory"
	},
	"require": {
		"doctrine/dbal": "^3.8",
		"jweiland/checkfaluploads": ">=4.0.0",
		"jweiland/glossary2": ">=6.0.0",
		"jweiland/maps2": ">=11.0.0",
		"typo3/cms-backend": "^12.4",
		"typo3/cms-core": "^12.4.16",
		"typo3/cms-extbase": "^12.4",
		"typo3/cms-fluid": "^12.4",
		"typo3/cms-scheduler": "^12.4"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "~2.42.0",
		"friendsofphp/php-cs-fixer": "^3.14",
		"phpstan/phpstan": "^1.10",
		"phpunit/phpunit": "^9.6",
		"roave/security-advisories": "dev-latest",
		"typo3/cms-dashboard": "^12.4",
		"typo3/cms-install": "^12.4",
		"typo3/cms-reports": "^12.4",
		"typo3/testing-framework": "^7.0.2"
	},
	"replace": {
		"typo3-ter/telephonedirectory": "self.version"
	},
	"autoload": {
		"psr-4": {
			"JWeiland\\Telephonedirectory\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"JWeiland\\Telephonedirectory\\Tests\\": "Tests"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "telephonedirectory",
			"web-dir": ".Build/web"
		}
	}
}
